programa {
  funcao inicio() {
    real valor1, valor2, soma, porcentagem_45

    escreva ("Veja a soma de 2 números com uma porcentagem de 45% delas:")
    leia (valor1)
    leia (valor2)
  
    soma = valor1 + valor2
    porcentagem_45 = soma * 0.45

    escreva ("O resultado deu:" ,porcentagem_45)
    
  }
}
